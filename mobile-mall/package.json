        "autoprefixer": "7.1.6",
        "axios": "^0.19.2",
        "babel-core": "^6.26.3",
        "babel-eslint": "7.2.3",
        "babel-jest": "20.0.3",
        "babel-loader": "7.1.2",
        "babel-plugin-transform-decorators-legacy": "^1.3.5",
        "babel-polyfill": "^6.26.0",
        "babel-preset-react-app": "^3.1.1",
        "babel-runtime": "^6.26.0",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "1.1.3",
        "crypto-js": "^3.1.9-1",
